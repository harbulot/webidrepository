<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>project233-webapp</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<context-param>
		<param-name>org.restlet.clients</param-name>
		<param-value>HTTP HTTPS CLAP FILE</param-value>
	</context-param>
	<servlet>
		<servlet-name>FoafDirectoryService</servlet-name>
		<servlet-class>org.restlet.ext.servlet.ServerServlet</servlet-class>

		<init-param>
			<param-name>org.restlet.application</param-name>
			<param-value>uk.ac.manchester.rcs.corypha.core.CoryphaRootApplication</param-value>
		</init-param>
		<init-param>
			<param-name>org.restlet.autoWire</param-name>
			<param-value>true</param-value>
		</init-param>

		<init-param>
			<param-name>corypha_modules</param-name>
			<param-value>uk.ac.manchester.rcs.bruno.project233.WebidModule</param-value>
		</init-param>
		<init-param>
			<param-name>corypha_config_ini_url</param-name>
			<param-value>war:///WEB-INF/corypha.ini</param-value>
		</init-param>
	</servlet>

	<servlet-mapping>
		<servlet-name>FoafDirectoryService</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>
</web-app>